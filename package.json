  },
  "bin": {
    "jsforce": "./bin/jsforce"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "co-prompt": "^1.0.0",
    "coffee-script": "^1.10.0",
    "commander": "^2.9.0",
    "csv-parse": "^1.1.1",
    "csv-stringify": "^1.0.4",
    "faye": "^1.2.0",
    "inherits": "^2.0.1",
    "lodash": "^4.11.1",
    "multistream": "^2.0.5",
    "open": "0.0.5",
